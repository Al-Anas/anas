#include<iostream>
#include<string>
using namespace std;
void Delete(string s,int x,int y)
{

cout<<"After Delete:"<<s.erase(x,y);



}
int main()
{
    string s1;
    int x,y;
    cout<<"Given String:";
    cin>>s1;
    cout<<"Give Position:";
    cin>>x;
    cout<<"Give Length:";
    cin>>y;

    Delete(s1,x,y);
    return 0;
}

